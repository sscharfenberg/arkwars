//- this file is generated by webpack via html-webpack-plugin. do not modify the pug template include.
<% if (htmlWebpackPlugin.options.meta.isProd) { %>
script(src="/public/assets/common.js" defer)
script(src="/public/assets/app.<%= htmlWebpackPlugin.files.chunks["app"].hash %>.js" defer)
<% } else { %>
script(src="http://localhost:<%= htmlWebpackPlugin.options.meta.webPackPort %>/webpack-dev-server.js" defer)
script(src="http://localhost:<%= htmlWebpackPlugin.options.meta.webPackPort %>/common.js" defer)
script(src="http://localhost:<%= htmlWebpackPlugin.options.meta.webPackPort %>/app.js" defer)
<% } %>
