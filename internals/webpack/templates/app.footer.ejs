<% if (htmlWebpackPlugin.options.meta.isProd) { %>
script(src="/public/assets/vendor.<%= htmlWebpackPlugin.files.chunks["vendor"].hash %>.js" defer nonce=(nonce))
script(src="/public/assets/common.<%= htmlWebpackPlugin.files.chunks["common"].hash %>.js" defer nonce=(nonce))
script(src="/public/assets/app.<%= htmlWebpackPlugin.files.chunks["app"].hash %>.js" defer nonce=(nonce))
<% } else { %>
script(src="//localhost:<%= htmlWebpackPlugin.options.meta.webPackPort %>/webpack-dev-server.js" defer nonce=(nonce))
script(src="//localhost:<%= htmlWebpackPlugin.options.meta.webPackPort %>/vendor.js" defer nonce=(nonce))
script(src="//localhost:<%= htmlWebpackPlugin.options.meta.webPackPort %>/common.js" defer nonce=(nonce))
script(src="//localhost:<%= htmlWebpackPlugin.options.meta.webPackPort %>/app.js" defer nonce=(nonce))
<% } %>
