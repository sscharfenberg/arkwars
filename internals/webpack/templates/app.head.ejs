//- this file is generated by webpack via html-webpack-plugin. do not modify the pug template include.
doctype html
html(lang="de")
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    title= h.appName
<% if (htmlWebpackPlugin.files.favicon) { %>
    link(href="<%= htmlWebpackPlugin.files.favicon %>" rel="shortcut icon")
<% } %>
<% if (htmlWebpackPlugin.options.meta.isProd) { %>
    <% for (key in htmlWebpackPlugin.files.css) { %>
    link(href="/<%= htmlWebpackPlugin.files.css[key].replace( /\.\.\//g, "" ) %>" rel="stylesheet")
    <% } %>
<% } %>
